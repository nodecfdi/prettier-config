{
  "name": "@nodecfdi/prettier-config",
  "description": "Configuraci√≥n Prettier para proyectos NodeCfdi",
  "version": "1.3.0",
  "type": "module",
  "main": "./dist/commonjs/base.js",
  "module": "./dist/esm/base.js",
  "types": "./dist/commonjs/base.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/base.d.ts",
        "default": "./dist/esm/base.js"
      },
      "require": {
        "types": "./dist/commonjs/base.d.ts",
        "default": "./dist/commonjs/base.js"
      }
    },
    "./adonis": {
      "import": {
        "types": "./dist/esm/adonis.d.ts",
        "default": "./dist/esm/adonis.js"
      },
      "require": {
        "types": "./dist/commonjs/adonis.d.ts",
        "default": "./dist/commonjs/adonis.js"
      }
    },
    "./pkg": {
      "import": {
        "types": "./dist/esm/pkg.d.ts",
        "default": "./dist/esm/pkg.js"
      },
      "require": {
        "types": "./dist/commonjs/pkg.d.ts",
        "default": "./dist/commonjs/pkg.js"
      }
    }
  },
  "scripts": {
    "prepare": "is-in-ci || husky",
    "typecheck": "tsc --noEmit",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "prebuild": "pnpm typecheck",
    "build": "tshy",
    "release": "np",
    "version": "pnpm run build && pnpm run changelog"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nodecfdi/tsconfig": "^1.7.2",
    "auto-changelog": "^2.5.0",
    "husky": "^9.1.6",
    "is-in-ci": "^1.0.0",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "tshy": "^3.0.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "prettier-edgejs": "^0.2.33"
  },
  "peerDependencies": {
    "prettier": "^3.3.3"
  },
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nodecfdi/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodecfdi/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/nodecfdi/prettier-config/issues"
  },
  "keywords": [
    "prettier",
    "nodecfdi",
    "shared",
    "config"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "pnpm@9.12.3",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "hide-credit": true
  },
  "np": {
    "message": "chore(release): :tada: %s",
    "tag": "latest",
    "branch": "main",
    "yolo": true
  },
  "tshy": {
    "selfLink": false,
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/base.ts",
      "./adonis": "./src/adonis.ts",
      "./pkg": "./src/pkg.ts"
    }
  }
}
